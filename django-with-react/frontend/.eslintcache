    ]}\n            hasFeedback\n            {...fieldErrors.username}\n            >\n                <Input />\n            </Form.Item>\n\n            <Form.Item\n                label=\"Password\"\n                name=\"password\"\n                rules={[{ required: true, message: 'Please input your password!' }]}\n                {...fieldErrors.password}\n            >\n                <Input.Password />\n            </Form.Item>\n\n            <Form.Item {...tailLayout}>\n                <Button type=\"primary\" htmlType=\"submit\">\n                Submit\n                </Button>\n            </Form.Item>\n        </Form>\n    );\n}\nconst layout = {\n    labelCol: { span: 8 },\n    wrapperCol: { span: 16 },\n  };\n\nconst tailLayout = {\n    wrapperCol: { offset: 8, span: 16 },\n};","/Users/ajyng/Desktop/alldaylongdjango/django-with-react/frontend/src/utils/useLocalStorage.js",[],"/Users/ajyng/Desktop/alldaylongdjango/django-with-react/frontend/src/store.js",["85"],"/Users/ajyng/Desktop/alldaylongdjango/django-with-react/frontend/src/utils/LoginRequiredRoute.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":3,"column":10,"nodeType":"92","messageId":"93","endLine":3,"endColumn":22},{"ruleId":"90","severity":1,"message":"94","line":11,"column":13,"nodeType":"92","messageId":"93","endLine":11,"endColumn":21},{"ruleId":"95","severity":1,"message":"96","line":29,"column":8,"nodeType":"97","endLine":29,"endColumn":10,"suggestions":"98"},{"ruleId":"86","replacedBy":"99"},{"ruleId":"88","replacedBy":"100"},{"ruleId":"90","severity":1,"message":"101","line":1,"column":27,"nodeType":"92","messageId":"93","endLine":1,"endColumn":36},{"ruleId":"90","severity":1,"message":"102","line":3,"column":10,"nodeType":"92","messageId":"93","endLine":3,"endColumn":15},{"ruleId":"90","severity":1,"message":"103","line":3,"column":38,"nodeType":"92","messageId":"93","endLine":3,"endColumn":46},{"ruleId":